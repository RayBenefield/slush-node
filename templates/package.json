{
    "name": "<%= appNameSlug %>",
    "description": "<%= appDescription %>",
    "version": "<%= appVersion %>",
    "homepage": "https://github.com/<%= userName %>/<%= appNameSlug %>",
    "author": {
        "name": "<%= authorName %>",
        "email": "<%= authorEmail %>"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/<%= userName %>/<%= appNameSlug %>.git"
    },
    "bugs": {
        "url": "https://github.com/<%= userName %>/<%= appNameSlug %>/issues"
    },
    "license": "<%= license %>",
    "main": "src/<%= appNameSlug %>.js",
    "engines": {
        "node": ">= 0.10.26",
        "npm": ">=1.4.3"
    },
    "scripts": {
        "test": "jest --watch",
        "lint": "eslint -c .eslintrc src/",
        "build": "babel src -d dist",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "config": "^1.21.0",
        "cross-env": "^3.1.3",
        "underscore": "^1.8.3",
    "underscore.string": "^2.4.0"
    },
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "babel-jest": "^17.0.2",
        "babel-polyfill": "^6.16.0",
        "babel-preset-es2015": "^6.18.0",
        "eslint": "^3.2.2",
        "eslint-config-airbnb": "^12.0.0",
        "eslint-plugin-react": "^6.3.0",
        "eslint-plugin-import": "^1.16.0",
        "eslint-plugin-jsx-a11y": "^2.2.3",
        "gulp": "^3.9.1",
        "gulp-cached": "^1.1.0",
        "gulp-debug": "^2.1.2",
        "gulp-if": "^2.0.1",
        "gulp-notify": "^2.2.0",
        "gulp-plumber": "^1.1.0",
        "gulp-shell": "^0.5.2",
        "jest": "^17.0.3",
        "rimraf": "^2.5.4"
    },
    "babel": {
        "presets": [ "es2015" ]
    },
    "jest": {
        "notify": true,
        "rootDir": "src",
        "testEnvironment": "node"
    }
}
